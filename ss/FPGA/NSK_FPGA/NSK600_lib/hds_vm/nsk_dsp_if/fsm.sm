  if adr=DEF_OSCH0+i*DEF_REGCNTCH+DEF_BRR then tx_dsp_int  <= brr_int(i); end if;
  if adr=DEF_OSCH0+i*DEF_REGCNTCH+DEF_OSR then tx_dsp_int  <= osr(i); end if;
end loop;

for i in 0 to CH_SYNC-1 loop
