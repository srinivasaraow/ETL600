  when DEF_IDR  => tx_dsp_int  <= DEF_VERSION;
  when DEF_TSIG  => tx_dsp_int  <= \"0000\" & tsig_nsk;
  when DEF_TUD1  => tx_dsp_int  <= tx_pos_1;
  when DEF_TUD2  => tx_dsp_int  <= tx_pos_2;
  when DEF_TUD3  => tx_dsp_int  <= tx_pos_3;
  when DEF_TUD4  => tx_dsp_int  <= tx_pos_4;
  when DEF_RUD1  => tx_dsp_int <= rx_pos_1_int;
  when DEF_RUD2  => tx_dsp_int  <= rx_pos_2_int;
  when DEF_RUD3  => tx_dsp_int  <= rx_pos_3_int;
  when DEF_RUD4  => tx_dsp_int  <= rx_pos_4_int;
  when DEF_RSIG  => tx_dsp_int <= \"0000\" & rsig_nsk_int; 
  when DEF_GDTC => tx_dsp_int <= \"00000\" & digtrans_c_int.gdtc.ric & digtrans_c_int.gdtc.rdc & digtrans_c_int.gdtc.mss; 
  when DEF_VCE0 =>  tx_dsp_int <= digtrans_c_int.vce(7 downto 0);
  when DEF_VCE1 =>  tx_dsp_int <= digtrans_c_int.vce(15 downto 8);
  when DEF_VCTL0 => tx_dsp_int <= digtrans_c_int.vctl(7 downto 0);
  when DEF_VCTL1 => tx_dsp_int <= digtrans_c_int.vctl(15 downto 8);
  when DEF_GDTS => tx_dsp_int <= \"0000000\" & status_digtrans.gdts.nsto;
  when DEF_TND0 => tx_dsp_int <= status_digtrans.tnd(7 downto 0);
  when DEF_TND1 => tx_dsp_int <= status_digtrans.tnd(15 downto 8);
  when DEF_FDCT => tx_dsp_int <= conv_std_logic_vector(status_digtrans.fdct, 8);
  when DEF_FICT => tx_dsp_int <= conv_std_logic_vector(status_digtrans.fict, 8);
  when DEF_FDCR => tx_dsp_int <= conv_std_logic_vector(status_digtrans.fdcr, 8);
  when DEF_FICR => tx_dsp_int <= conv_std_logic_vector(status_digtrans.ficr, 8);
--  when others => tx_dsp_int <= X\"AF\";
  when others => NULL;
end case;

for i in 0 to 10 loop
  if adr=DEF_OSCH0+i*DEF_REGCNTCH+DEF_PCR then tx_dsp_int  <= pcr_int(i); end if;
  if adr=DEF_OSCH0+i*DEF_REGCNTCH+DEF_CCR then tx_dsp_int <= ccr_int(i); end if;
